<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>Library</OutputType>
    <RootNamespace>MongoMigrations</RootNamespace>
    <AssemblyName>MongoMigrations</AssemblyName>
	<TargetFrameworks>netcoreapp2.0;netstandard2.0;net462;net47</TargetFrameworks>
    <Version>2.4.4-1</Version>
    <AssemblyVersion>2.4.4.1</AssemblyVersion>
	<FileVersion>2.4.4.1</FileVersion>
	<LangVersion>latest</LangVersion>
	<Authors>YouPark AS</Authors>
	<Company>YouPark AS</Company>
	<Copyright>YouPark AS</Copyright>
    <Description>Mongo migrations for dot net developers</Description>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
	<PackageLicenseUrl>https://github.com/youpark/youparkas.common.infrastructure</PackageLicenseUrl>
	<PackageProjectUrl>https://github.com/youpark/youparkas.common.infrastructure</PackageProjectUrl>
	<RepositoryUrl>https://github.com/youpark/youparkas.common.infrastructure</RepositoryUrl>
	<RepositoryType>git</RepositoryType>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="MongoDB.Bson" Version="2.4.4" />
    <PackageReference Include="MongoDB.Driver" Version="2.4.4" />
    <PackageReference Include="MongoDB.Driver.Core" Version="2.4.4" />
    <PackageReference Include="System.Runtime.InteropServices.RuntimeInformation" Version="4.3.0" />
  </ItemGroup>
  <ItemGroup Condition="'$(TargetFramework)' == 'net462' or '$(TargetFramework)' == 'net47'">
    <Reference Include="System" />
    <Reference Include="System.Core" />
  </ItemGroup>
</Project>